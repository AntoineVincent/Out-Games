<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}OutGames{% endblock %}</title>
        <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/style.css') }}">
        <script type="text/javascript" src="{{ asset('js/jquery.js') }}" ></script>
        <script type="text/javascript" src="{{ asset('js/bootstrap.min.js') }}" ></script>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link href='https://fonts.googleapis.com/css?family=Aldrich' rel='stylesheet' type='text/css'>
    </head>
    <body>
        <nav class="navbar navbar-default navbar-custom" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
              <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              
            </div>
            <a class="navbar-brand menu-title" href="#">Out Games</a>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="navbar-collapse-1">
             
              <ul class="nav navbar-nav navbar-left">
                <li><a target="blank" class="menu" href="https://www.facebook.com/pokemongoEureetLoir">Facebook</a></li>
                <li><a target="blank" class="menu" href="https://twitter.com/PokeGoEureLoir">Twitter</a></li>
              </ul>
              <ul class="nav navbar-nav navbar-right">
                <li><a class="menu" href="#">PokemonGo</a></li>
                <li><a class="menu" href="#">Another</a></li>
                <li><a class="menu" href="#">F.A.Q</a></li>
              </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
            <div class="col-md-12 partenaire-div">
                <div class="col-md-12">
                    <h4 class="parte-title">Nos Partenaires</h4>
                </div>
                <div class="col-md-12">
                    <div class="col-md-4"></div>
                    <div class="col-md-4">
                        <div class="col-md-4">
                            <a target="blank" href="http://www.redbull.com/fr/fr"><img src="{{ asset('img/redbull.png') }}" alt="partenaire redbull" class="logo-partenaire"></a>
                        </div>
                        <div class="col-md-4">
                            <a target="blank" href="www.millenium.org"><img src="{{ asset('img/Millenium_E-Sport_Logo.png') }}" alt="partenaire millenium" class="logo-partenaire"></a>
                        </div>
                        <div class="col-md-4">
                            <a target="blank" href="https://www.camara.net/chartres"><img src="{{ asset('img/camara.png') }}" alt="partenaire camara" class="logo-partenaire"></a>
                        </div>
                    </div>
                    <div class="col-md-4"></div>
                </div>
            </div>

        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
